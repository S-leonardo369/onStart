{
    "manifest_version": 3,
    "name": "onStart",
    "version": "1.0",
    "permissions": [
        "tabs",
        "storage"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icons/icon1.png",
            "48": "icons/icon2.png",
            "128": "icons/icon3.png"
        }
    },
    "icons": {
        "16": "icons/icon1.png",
        "48": "icons/icon2.png",
        "128": "icons/icon3.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}